
CREATE TABLE E_4
(
	aaa                  CHAR(18)  NOT NULL ,
	bbb                  CHAR(18)  NULL 
);

CREATE UNIQUE INDEX XPKE_4 ON E_4
(aaa   ASC);

ALTER TABLE E_4
	ADD CONSTRAINT  XPKE_4 PRIMARY KEY (aaa);

CREATE TABLE GITONE
(
	a                    CHAR(18)  NOT NULL ,
	b                    CHAR(18)  NULL 
);

CREATE UNIQUE INDEX XPKGITONE ON GITONE
(a   ASC);

ALTER TABLE GITONE
	ADD CONSTRAINT  XPKGITONE PRIMARY KEY (a);

CREATE TABLE GITTWO
(
	d                    CHAR(18)  NOT NULL ,
	e                    CHAR(18)  NULL 
);

CREATE UNIQUE INDEX XPKGITTWO ON GITTWO
(d   ASC);

ALTER TABLE GITTWO
	ADD CONSTRAINT  XPKGITTWO PRIMARY KEY (d);
