
CREATE TABLE IDENTIFIER('CESAR')
(
	Aaa CHAR(18) NOT NULL ,
	Bbb CHAR(18) NULL 
);

CREATE TABLE IDENTIFIER('HHHHHH')
(
	Adfsf CHAR(18) NOT NULL ,
	Dfsdfs CHAR(18) NULL 
);

ALTER TABLE CESAR
	ADD CONSTRAINT XPKCESAR PRIMARY KEY (Aaa);

ALTER TABLE HHHHHH
	ADD CONSTRAINT XPKHHHHHH PRIMARY KEY (Adfsf);
